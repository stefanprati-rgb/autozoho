                "css": "h2.modal-title",
                "xpath": "//h2[contains(@class, 'modal-title')]"
            },
            "botoes": {
                "confirmar": {
                    "css": "button.confirm",
                    "xpath": "//button[contains(@class, 'confirm')]"
